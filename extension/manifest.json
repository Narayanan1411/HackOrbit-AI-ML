{
    "manifest_version": 3,
    "name": "URL Auto Fetcher with Display",
    "version": "1.0",
    "description": "Automatically sends visited URLs to an API and shows current URL in popup",
    "permissions": [
      "tabs",
      "activeTab",
      "scripting"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_title": "URL Auto Fetcher"
    },
    "host_permissions": [
      "<all_urls>"
    ]
  }
  